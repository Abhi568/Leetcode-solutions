class Solution:
    def findPairs(self, nums: List[int], k: int) -> int:
        nums.sort()
        count=0
        setn=set()
        i,j=0,0
        while(j<len(nums)):
            if i==j:
                j+=1
            elif nums[j]-nums[i]==k and (nums[i],nums[j]) not in setn:
                count+=1
                setn.add((nums[i],nums[j]))
                i+=1
                j+=1
            elif nums[j]-nums[i]<k:
                j+=1
            else:
                i+=1
        return count
    # [1,1,3,4,5] k=2
    # [1,2,3,4,1,2,3,4,5,1,2,3,5,6,7,8]
    # [1,1,1,2,2,2,3,3,3,4,4,5,6,6,7,8]
